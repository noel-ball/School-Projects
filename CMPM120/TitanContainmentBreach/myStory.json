            "Body": "Cronus Administrator Control",
            "Choices": [
                {
                    "Text": "Access Cronus",
                    "Target": "Login"
                },
                {
                    "Text": "Data-Base",
                    "Target": "Data"
                }
            ]
        },
        "Data": {
            "Body": "<hr>Page One:<br>Unable, Unafraid, Unfair, Unimpressed, Unlimited, Unspoken<br><br>Page Two:<br>Devout, Divine, Holy, Righteous, Sacred, Spiritual<br><br>Page Three:<br>Angrily, Bravely, Partialy, Proudly, Quickly, Suddenly<br><br>Possible Passwords:<br>Overcharged, Unfaithful, Religiously, Unknown, Ungodly, Quietly, Uneasily, Refortify<hr>",
            "Choices": [
                {
                    "Text": "Go Back",
                    "Target": "Admin"
                }
            ]
        },
        "Login": {
            "Body": "Enter Password:",
            "Guesses": ["Overcharged", "Unfaithful", "Religiously", "Unknown", "Ungodly", "Quietly", "Uneasily", "Refortify"],
